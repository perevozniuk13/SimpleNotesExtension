{
    "name": "Simple Notes Extension",
    "description": "Simple extension designed to help you capture your thoughts, ideas, and tasks instantly.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
    "scripting",
    "storage",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "type": "module"
    }
  ]
  }